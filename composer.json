{
    "name": "vladmeh/xml-utils",
    "version": "1.1.1",
    "description": "Xml Utils",
    "license": "MIT",
    "keywords": ["laravel", "xml"],
    "homepage": "https://github.com/vladmeh/xml-utils",
    "authors": [
        {
            "name": "Mikhaylov.V",
            "email": "mihailov.v@fhclub.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Vladmeh\\XmlUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vladmeh\\XmlUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vladmeh\\XmlUtils\\XmlUtilsServiceProvider"
            ]
        }
    }
}
