{
    "name": "vladmeh/xml-utils",
    "description": "Xml Utils",
    "license": "MIT",
    "keywords": ["php", "xml", "utils"],
    "homepage": "https://github.com/vladmeh/xml-utils",
    "authors": [
        {
            "name": "Mikhaylov.V",
            "email": "mihailov.v@fhclub.ru"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Vladmeh\\XmlUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vladmeh\\XmlUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
